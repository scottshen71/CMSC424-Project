<html>
<head>
  <meta charset="utf-8">
  <link href="css/style1.css" rel="stylesheet">

</head>
<body>
<div class="column-layout">
  <div class="main-column">
  <button onclick="goBack()">Go Back</button>

<script>
function goBack() {
    window.history.back();
}
</script>
    <h2>Main Table V2</h2>
	
		
		<table id = "head">
		<% for(var i=0; i < title.length; i++) { %>
		   <tr>
			 <td><%= title[i].id %></td>
			 <td><%= title[i].name %></td>
		   </tr>
		<% } %>
		</table>
